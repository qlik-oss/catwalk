{
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["import", { "libraryName": "hamus.js", "libraryDirectory": "src"}] 
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": "last 2 versions"
            }
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-react-jsx",
        "istanbul"
      ]
    }
  }
}
