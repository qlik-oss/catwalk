{
  "name": "catwalk",
  "version": "1.0.0",
  "description": "Get Qlik Associative Engine data model insights.",
  "author": "QlikTech International AB",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qlik-oss/catwalk"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production parcel build src/index.html",
    "parcel:build": "cross-env NODE_ENV=test parcel build src/index.html --no-minify",
    "start": "parcel src/index.html --open",
    "server:start": "http-server ./dist/ -p 1234",
    "test:unit": "aw --glob test/unit/*.spec.js --src src/logic/*.js --coverage",
    "test:comp": "aw -c ./test/comp/aw.config.js && nyc report -t ./coverage/.nyc_output -r lcov -r text",
    "test:e2e": "aw puppeteer -c ./test/e2e/aw.config.js",
    "test:e2e:dev": "cross-env CI=true aw puppeteer -c ./test/e2e/aw.config.local.js",
    "coverage:html": "nyc report -t ./coverage/.nyc_output -r html",
    "lint": "eslint --ext .js,.jsx . && stylelint src/.",
    "lint:fix": "eslint --ext .js,.jsx --fix . && stylelint --fix src/."
  },
  "dependencies": {
    "axios": "0.21.1",
    "axios-cache-adapter": "2.7.3",
    "axios-hooks": "2.6.3",
    "copy-to-clipboard": "3.3.1",
    "enigma.js": "2.7.3",
    "hamus.js": "0.3.0",
    "prop-types": "15.7.2",
    "re-resizable": "6.9.0",
    "react": "17.0.2",
    "react-collapsible": "2.8.3",
    "react-contexify": "5.0.0",
    "react-cookie-consent": "6.2.4",
    "react-dom": "17.0.2",
    "react-floater": "0.7.3",
    "react-ga": "3.3.0",
    "react-joyride": "2.2.1",
    "react-outside-click-handler": "1.3.0",
    "react-svg-inline": "2.1.1",
    "react-tabs": "3.2.2",
    "react-use-promise": "0.3.1",
    "react-virtualized": "9.22.3",
    "react-infinite-scroller": "1.2.4"
  },
  "devDependencies": {
    "@after-work.js/cli": "6.0.14",
    "@after-work.js/node": "6.0.14",
    "@after-work.js/puppeteer": "6.0.14",
    "@after-work.js/serve": "6.0.14",
    "@babel/core": "7.14.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-transform-async-to-generator": "7.14.5",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.9",
    "@babel/preset-react": "7.14.5",
    "autoprefixer": "10.3.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-istanbul": "6.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "fs-extra": "10.0.0",
    "http-server": "0.12.3",
    "jsdom": "16.7.0",
    "nyc": "15.1.0",
    "parcel-bundler": "1.12.5",
    "parcel-plugin-inlinesvg": "0.1.1",
    "postcss-nested": "4.2.3",
    "raf": "3.4.1",
    "react-test-renderer": "17.0.2",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0"
  }
}
